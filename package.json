{
  "name": "executive-ai-solutions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run build:css\" \"next dev\"",
    "build": "npm run build:css:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "build:css": "npx @tailwindcss/cli -i ./input.css -o ./app/output.css --watch",
    "build:css:prod": "npx @tailwindcss/cli -i ./input.css -o ./app/output.css --minify"
  },
  "dependencies": {
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@sanity/client": "^7.8.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^4.3.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.9",
    "next": "15.4.3",
    "next-sanity": "^10.0.10",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^4.7.0",
    "sanity": "^4.3.0",
    "tailwindcss": "^4.1.11",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@sanity/mcp-server": "^0.12.1",
    "@tailwindcss/cli": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "typescript": "^5"
  }
}
